#include <iostream>
#include<cmath>
using namespace std;

int main() {
	long long int N=1000001;
	long long int prefix[1000001]={0};
	 int m=pow(10,9)+7;
	prefix[1]=1;
	for(int i=2;i<=N;i++){
	    prefix[i]=((prefix[i-1]*i)%m);
	}
	    int t;
	    cin>>t;
	 
	  for(int i=t;i>=0;i--){
	     int n;
	      cin>>n;
	      if(n==1)
	       cout<<(1%m)<<"\n";
	      else
	          cout<<prefix[n]<<"\n";
	  }
	
return 0;
}
